[Unit]
Description=OSMRI
Requires=network-online.target
After=network-online.target
Before=

[Service]
Type=simple

WorkingDirectory=_OSMRI_GIT_DIR_
ExecStartPre=_OSMRI_GIT_DIR_/src/src_caninterface/init_usbtin.sh

ExecStart=_OSMRI_GIT_DIR_/src/src_caninterface/start_can2rest.sh
